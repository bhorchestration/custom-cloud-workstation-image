{
    "workbench.startupEditor": "newUntitledFile",
    "go.formatTool": "goimports",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "editor.minimap.enabled": false,
    "explorer.confirmDelete": false,
    "cSpell.userWords": [
        "commandline",
        "Grafeas",
        "Kaniko",
        "kube",
        "KUBECONFIG",
        "kubectl",
        "Kubernetes",
        "prefs",
        "unmarshalling",
        "upsert",
        "Vulns"
    ],
    "editor.wordWrap": "on",
    "explorer.confirmDragAndDrop": false,
    "go.useLanguageServer": true,
    "git.followTagsWhenSync": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "git.ignoreRebaseWarning": true,
    "go.toolsManagement.autoUpdate": true,
    "docker.showStartPage": false,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.fontSize": 16,
    "redhat.telemetry.enabled": false,
    "workbench.editor.untitled.hint": "hidden",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.inlineSuggest.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "cSpell.language": "en,en-US",
    "git.autofetch": true,
    "makefile.extensionOutputFolder": "",
    "workbench.iconTheme": "Monokai Classic Monochrome Icons",
    "telemetry.telemetryLevel": "off",
    "workbench.colorCustomizations": {
        "statusBar.background" : "#333",
        "statusBar.noFolderBackground" : "#333",
        "statusBar.debuggingBackground": "#333"
    },
    "workbench.colorTheme": "Monokai",
    "cloudcode.updateChannel": "Insiders",
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "workbench.editor.enablePreview": false,
    "cSpell.autoFormatConfigFile": true,
    "cSpell.blockCheckingWhenAverageChunkSizeGreaterThan": 200,
    "go.autocompleteUnimportedPackages": true,
    "window.zoomLevel": 2
}